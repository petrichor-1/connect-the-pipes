							list = Self.â†©ï¸ğŸ“Šremoved_list
							increase(new_offset, by: 1)
						custom_block rebuild(list: list)
						Game.generated_levels = Self.â†©ï¸ğŸ“Šrebuilt_list
						Game.generated_levels_list_offset = new_offset

